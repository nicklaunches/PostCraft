# PostCraft Database URL for PostgreSQL connection
# Example: postgresql://user:password@localhost:5432/postcraft
POSTCRAFT_DATABASE_URL=

# PostCraft Studio Port (default: 3579)
# Binds to 127.0.0.1:3579 for localhost-only access
POSTCRAFT_PORT=3579

# Unlayer Email Editor Project ID
# Get your project ID from https://console.unlayer.com/projects
NEXT_PUBLIC_UNLAYER_PROJECT_ID=
